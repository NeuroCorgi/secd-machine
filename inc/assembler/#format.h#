#ifndef SECD_ASM_FORMAT_H
#define SECD_ASM_FORMAT_H

#include <iostream>

#include "common.h"

#include "vm/value.h"
#include "assembler/value.h"

std::istream& operator>>(std::istream& is, secd::value::operands_list& ops) {
    char paren;
    is >> paren;
    if (paren != '(') {
        throw std::runtime_error("Operands list expected to start with '('");
    }

